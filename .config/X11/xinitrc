#!/bin/bash

apps=(
	# 'mopidy'
	'dwmblocks'
  'picom'
	# 'dropbox'
)

for app in ${apps[@]}; do
	pgrep="$(pgrep $app)"
	if [[ $pgrep == "" ]]; then
		eval $app &
	fi
done

pkill redshift
redshift &
feh --no-fehbg --bg-fill -z $HOME/Pictures/nord
setxkbmap -option caps:escape
while type dwm >/dev/null ; do dbus-launch --exit-with-session dwm && continue || break ; done
